{
  "name": "mentalspace-ehr",
  "version": "1.0.0",
  "description": "MentalSpace EHR - A comprehensive electronic health record system",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=frontend & npm run start:dev --workspace=backend",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:backend": "npm run start:dev --workspace=backend",
    "build": "npm run build --workspaces",
    "build:frontend": "npm run build --workspace=frontend",
    "build:backend": "npm run build --workspace=backend",
    "install:all": "npm install",
    "lint": "npm run lint --workspaces",
    "lint:frontend": "npm run lint --workspace=frontend",
    "lint:backend": "npm run lint --workspace=backend",
    "test": "npm run test --workspaces",
    "test:frontend": "npm run test --workspace=frontend",
    "test:backend": "npm run test --workspace=backend",
    "prisma:generate": "npm run prisma:generate --workspace=backend",
    "prisma:migrate": "npm run prisma:migrate --workspace=backend",
    "prisma:studio": "npm run prisma:studio --workspace=backend",
    "db:seed": "npm run db:seed --workspace=backend",
    "clean": "npm run clean --workspaces",
    "clean:node_modules": "rm -rf node_modules apps/*/node_modules packages/*/node_modules",
    "clean:dist": "rm -rf apps/*/dist apps/*/build packages/*/dist packages/*/build"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "ehr",
    "healthcare",
    "mental-health",
    "react",
    "nestjs",
    "typescript"
  ],
  "author": "",
  "license": "UNLICENSED"
} 